local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("SalmonTooI", "Ocean")
local Tab = Window:NewTab("General")
local VisualExploits = Tab:NewSection("Visual Exploits")
----BREAK

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local LocalPlayer = Players.LocalPlayer

local drawnObjects = {}
local drawingEnabled = false

local function clearDrawnObjects()
    for _, obj in pairs(drawnObjects) do
        if obj and obj.Parent then
            obj:Destroy()
        end
    end
    drawnObjects = {}
end

local function drawBoxAroundPlayer(player, color)
    local torso = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
    if not torso then return end

    local box = Instance.new("BoxHandleAdornment")
    box.Size = Vector3.new(4, 6, 2) -- Adjust size as needed
    box.Color3 = color
    box.Adornee = torso
    box.ZIndex = 10
    box.AlwaysOnTop = true
    box.Parent = torso
    box.Transparency = 0.5 -- Set transparency to make it unfilled

    table.insert(drawnObjects, box)
end

local function drawLine(startPosition, endPosition, color, thickness)
    local line = Instance.new("Part")
    line.Anchored = true
    line.CanCollide = false
    line.Size = Vector3.new(thickness, thickness, (startPosition - endPosition).Magnitude)
    line.CFrame = CFrame.new(startPosition, endPosition) * CFrame.new(0, 0, -line.Size.Z / 2)
    line.Color = color
    line.Material = Enum.Material.Neon
    line.Parent = workspace
    table.insert(drawnObjects, line)
end

local function drawLinesAndBoxes()
    if not drawingEnabled then
        clearDrawnObjects()
        return
    end

    clearDrawnObjects()

    local torso = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
    if not torso then return end

    for _, player in pairs(Players:GetPlayers()) do
        if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            local otherTorso = player.Character.HumanoidRootPart
            local color = player.TeamColor.Color
            drawLine(torso.Position, otherTorso.Position, color, 0.1) -- Draw line to player
            drawBoxAroundPlayer(player, color) -- Draw box around player
        end
    end
end

local function toggleDrawing()
    drawingEnabled = not drawingEnabled
end

RunService.RenderStepped:Connect(drawLinesAndBoxes)

VisualExploits:NewButton("ESP", "Boxes Players, Displays health and their username", function()
    toggleDrawing() -- Toggle ESP on button click
end)

VisualExploits:NewButton("Remove GUI's", "Remove the GUI's", function()

-- LocalScript placed in StarterPlayerScripts

local player = game.Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

-- Function to remove all GUI elements
local function removeAllGUIs()
    for _, gui in ipairs(playerGui:GetChildren()) do
        if gui:IsA("ScreenGui") then
            gui:Destroy()
        end
    end
end

-- Remove GUIs when the player joins the game
removeAllGUIs()

-- Optionally, you can also handle GUI removal when the player respawns
player.CharacterAdded:Connect(function()
    removeAllGUIs()
end)

end)

