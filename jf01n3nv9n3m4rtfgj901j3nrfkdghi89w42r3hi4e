local joinTeamEnabled = false

SectionFE:NewButton("JoinAnyAvailableTeam", "Type in the team name to join", function()
    joinTeamEnabled = not joinTeamEnabled -- Toggle join team functionality

    if joinTeamEnabled then
        local screenGui = Instance.new("ScreenGui")
        screenGui.Name = "TeamJoinGui"
        screenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
        screenGui.Enabled = true

        local frame = Instance.new("Frame")
        frame.Size = UDim2.new(0, 200, 0, 100)
        frame.Position = UDim2.new(1, -210, 0.5, -50)
        frame.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
        frame.BorderSizePixel = 0
        frame.BackgroundTransparency = 0.5
        frame.Parent = screenGui

        local uiCorner = Instance.new("UICorner")
        uiCorner.CornerRadius = UDim.new(0, 10)
        uiCorner.Parent = frame

        local textBox = Instance.new("TextBox")
        textBox.Size = UDim2.new(0, 180, 0, 30)
        textBox.Position = UDim2.new(0, 10, 0, 10)
        textBox.Font = Enum.Font.Gotham
        textBox.PlaceholderText = "Enter Team Name"
        textBox.TextColor3 = Color3.fromRGB(255, 255, 255)
        textBox.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        textBox.BorderSizePixel = 0
        textBox.Parent = frame

        local uiCornerTextbox = Instance.new("UICorner")
        uiCornerTextbox.CornerRadius = UDim.new(0, 5)
        uiCornerTextbox.Parent = textBox

        local joinButton = Instance.new("TextButton")
        joinButton.Size = UDim2.new(0, 180, 0, 30)
        joinButton.Position = UDim2.new(0, 10, 0, 50)
        joinButton.Font = Enum.Font.Gotham
        joinButton.Text = "Join Team"
        joinButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        joinButton.BackgroundColor3 = Color3.fromRGB(0, 100, 255)
        joinButton.BorderSizePixel = 0
        joinButton.Parent = frame

        local uiCornerButton = Instance.new("UICorner")
        uiCornerButton.CornerRadius = UDim.new(0, 5)
        uiCornerButton.Parent = joinButton

        local function joinTeam()
            local teamName = textBox.Text
            local player = game.Players.LocalPlayer

            for _, team in pairs(game.Teams:GetChildren()) do
                if team.Name == teamName then
                    player.Team = team
                    screenGui:Destroy() -- Remove the GUI entirely after joining
                    return
                end
            end

            warn("Team not found: " .. teamName)
        end

        joinButton.MouseButton1Click:Connect(joinTeam)

    else
        -- If toggled off, remove the GUI if it exists
        local existingGui = game.Players.LocalPlayer.PlayerGui:FindFirstChild("TeamJoinGui")
        if existingGui then
            existingGui:Destroy()
        end
    end
end)

