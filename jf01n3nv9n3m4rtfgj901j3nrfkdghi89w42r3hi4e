SectionFE:NewButton("TP to people forever", "Teleports forever", function()

-- Define a function to teleport the player to each other player in the game
local function teleportToPlayers()
    -- Get the local player
    local localPlayer = game.Players.LocalPlayer
    -- Loop infinitely
    while true do
        -- Iterate over all players in the game
        for _, player in pairs(game.Players:GetPlayers()) do
            -- Make sure the player is not the local player
            if player ~= localPlayer then
                -- Check if the player's character exists and has a HumanoidRootPart
                if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                    -- Teleport the local player's character to the other player's HumanoidRootPart
                    localPlayer.Character:SetPrimaryPartCFrame(player.Character.HumanoidRootPart.CFrame)
                    -- Optional: Add a very short delay to make the teleportation rapid but not instant
                    wait(0.01) -- Adjust the delay as needed
                end
            end
        end
    end
end

-- Call the function to start teleporting
teleportToPlayers()


end)

SectionFE:NewButton("Invisibility", "Kills You But invs.", function()

--[[Invisibility Toggle

You can find the original concept here: https://v3rmillion.net/showthread.php?tid=544634

This method clones the character locally, teleports the real character to a safe location, then sets the character to the clone.
Basically, your real character is in the sky while you are on the ground.

Because of the way this works, games with a decent anti-cheat might mess this up.
If you turn it off, you have to go to a safe place before going invisible.

Written by: BitingTheDust ; https://v3rmillion.net/member.php?action=profile&uid=1628149
]]

-- Settings:
local ScriptStarted = false
local Keybind = "E" -- Set to whatever you want, has to be the name of a KeyCode Enum.
local Transparency = true -- Will make you slightly transparent when you are invisible. No reason to disable.
local NoClip = false -- Will make your fake character no clip.

local Player = game:GetService("Players").LocalPlayer
local RealCharacter = Player.Character or Player.CharacterAdded:Wait()

local IsInvisible = false

RealCharacter.Archivable = true
local FakeCharacter = RealCharacter:Clone()
local Part = Instance.new("Part", workspace)
Part.Anchored = true
Part.Size = Vector3.new(200, 1, 200)
Part.CFrame = CFrame.new(0, -500, 0) -- Set this to whatever you want, just far away from the map.
Part.CanCollide = true
FakeCharacter.Parent = workspace
FakeCharacter.HumanoidRootPart.CFrame = Part.CFrame * CFrame.new(0, 5, 0)

-- Disable scripts in FakeCharacter
for _, v in pairs(RealCharacter:GetChildren()) do
    if v:IsA("LocalScript") then
        local clone = v:Clone()
        clone.Disabled = true
        clone.Parent = FakeCharacter
    end
end

-- Set transparency if enabled
if Transparency then
    for _, v in pairs(FakeCharacter:GetDescendants()) do
        if v:IsA("BasePart") then
            v.Transparency = 0.7
        end
    end
end

local CanInvis = true

-- Function to handle character death
local function RealCharacterDied()
    CanInvis = false
    RealCharacter:Destroy()
    RealCharacter = Player.Character
    CanInvis = true
    IsInvisible = false
    FakeCharacter:Destroy()
    workspace.CurrentCamera.CameraSubject = RealCharacter.Humanoid

    RealCharacter.Archivable = true
    FakeCharacter = RealCharacter:Clone()
    Part:Destroy()
    Part = Instance.new("Part", workspace)
    Part.Anchored = true
    Part.Size = Vector3.new(200, 1, 200)
    Part.CFrame = CFrame.new(9999, 9999, 9999) -- Set this to whatever you want, just far away from the map.
    Part.CanCollide = true
    FakeCharacter.Parent = workspace
    FakeCharacter.HumanoidRootPart.CFrame = Part.CFrame * CFrame.new(0, 5, 0)

    for _, v in pairs(RealCharacter:GetChildren()) do
        if v:IsA("LocalScript") then
            local clone = v:Clone()
            clone.Disabled = true
            clone.Parent = FakeCharacter
        end
    end

    if Transparency then
        for _, v in pairs(FakeCharacter:GetDescendants()) do
            if v:IsA("BasePart") then
                v.Transparency = 0.7
            end
        end
    end

    RealCharacter.Humanoid.Died:Connect(function()
        RealCharacter:Destroy()
        FakeCharacter:Destroy()
    end)

    Player.CharacterAppearanceLoaded:Connect(RealCharacterDied)
end

RealCharacter.Humanoid.Died:Connect(function()
    RealCharacter:Destroy()
    FakeCharacter:Destroy()
end)

Player.CharacterAppearanceLoaded:Connect(RealCharacterDied)

local PseudoAnchor
game:GetService("RunService").RenderStepped:Connect(function()
    if PseudoAnchor then
        PseudoAnchor.CFrame = Part.CFrame * CFrame.new(0, 5, 0)
    end
    if NoClip then
        FakeCharacter.Humanoid:ChangeState(11)
    end
end)

PseudoAnchor = FakeCharacter.HumanoidRootPart

-- Toggle invisibility
local function Invisible()
    if not IsInvisible then
        local StoredCF = RealCharacter.HumanoidRootPart.CFrame
        RealCharacter.HumanoidRootPart.CFrame = FakeCharacter.HumanoidRootPart.CFrame
        FakeCharacter.HumanoidRootPart.CFrame = StoredCF
        RealCharacter.Humanoid:UnequipTools()
        Player.Character = FakeCharacter
        workspace.CurrentCamera.CameraSubject = FakeCharacter.Humanoid
        PseudoAnchor = RealCharacter.HumanoidRootPart

        for _, v in pairs(FakeCharacter:GetChildren()) do
            if v:IsA("LocalScript") then
                v.Disabled = false
            end
        end

        IsInvisible = true
    else
        local StoredCF = FakeCharacter.HumanoidRootPart.CFrame
        FakeCharacter.HumanoidRootPart.CFrame = RealCharacter.HumanoidRootPart.CFrame
        RealCharacter.HumanoidRootPart.CFrame = StoredCF
        FakeCharacter.Humanoid:UnequipTools()
        Player.Character = RealCharacter
        workspace.CurrentCamera.CameraSubject = RealCharacter.Humanoid
        PseudoAnchor = FakeCharacter.HumanoidRootPart

        for _, v in pairs(FakeCharacter:GetChildren()) do
            if v:IsA("LocalScript") then
                v.Disabled = true
            end
        end

        IsInvisible = false
    end
end

-- Listen for keybind
game:GetService("UserInputService").InputBegan:Connect(function(key, gamep)
    if gamep then return end
    if key.KeyCode.Name:lower() == Keybind:lower() and CanInvis and RealCharacter and FakeCharacter then
        if RealCharacter:FindFirstChild("HumanoidRootPart") and FakeCharacter:FindFirstChild("HumanoidRootPart") then
            Invisible()
        end
    end
end)

-- Play sound and notify user
local Sound = Instance.new("Sound", game:GetService("SoundService"))
Sound.SoundId = "rbxassetid://232127604"
Sound:Play()
game:GetService("StarterGui"):SetCore("SendNotification", {
    Title = "Invisible Toggle Loaded",
    Text = "Press " .. Keybind .. " to change visibility.",
    Duration = 20,
    Button1 = "Okay."
})

end)

SectionFE:NewButton("Ghost Flight", "Flys like a ghost", function()

-- Variables
local Player = game.Players.LocalPlayer
local Char = Player.Character or Player.CharacterAdded:Wait()
local Humanoid = Char:WaitForChild("Humanoid")
local Root = Char:FindFirstChild("HumanoidRootPart")
local GhostMode = false
local TeleportToBodyMode = false
local LastGhostPosition = Root.Position

if not Char or not Root then
    warn("Character or HumanoidRootPart not found")
    return
end

-- Function to create or update the status indicator
local function updateStatusIndicator()
    local screenGui = Player:WaitForChild("PlayerGui"):FindFirstChild("TeleportToBodyStatus") or Instance.new("ScreenGui", Player.PlayerGui)
    screenGui.Name = "TeleportToBodyStatus"

    local indicator = screenGui:FindFirstChild("StatusIndicator") or Instance.new("Frame", screenGui)
    indicator.Name = "StatusIndicator"
    indicator.Size = UDim2.new(0, 20, 0, 20)
    indicator.Position = UDim2.new(1, -30, 0, 10)
    indicator.BackgroundColor3 = TeleportToBodyMode and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 0, 0)
    indicator.BorderSizePixel = 0
end

-- Function to enable or disable ghost mode
local function toggleGhostMode()
    GhostMode = not GhostMode
    if GhostMode then
        for _, part in pairs(Char:GetChildren()) do
            if part:IsA("BasePart") then
                part.Transparency = 0.5
            end
        end
        Humanoid.PlatformStand = true
        Root.Anchored = true
        LastGhostPosition = Root.Position
    else
        if TeleportToBodyMode then
            local glideSpeed = 0.1
            local targetPos = LastGhostPosition
            while (Root.Position - targetPos).Magnitude > 0.1 do
                Root.CFrame = CFrame.new(Root.Position:Lerp(targetPos, glideSpeed))
                wait(1/44)
            end
        end
        for _, part in pairs(Char:GetChildren()) do
            if part:IsA("BasePart") then
                part.Transparency = 0
            end
        end
        Humanoid.PlatformStand = false
        Root.Anchored = false
    end
end

-- Function to enable or disable TeleportToBody mode
local function toggleTeleportToBody()
    TeleportToBodyMode = not TeleportToBodyMode
    updateStatusIndicator()
end

-- Initial status indicator update
updateStatusIndicator()

-- Reapply TeleportToBody mode after death
Player.CharacterAdded:Connect(function(character)
    Char = character
    Humanoid = Char:WaitForChild("Humanoid")
    Root = Char:FindFirstChild("HumanoidRootPart") or Char:WaitForChild("HumanoidRootPart")
    if GhostMode then
        toggleGhostMode()
    end
    if TeleportToBodyMode then
        updateStatusIndicator()
    end
end)

-- Flying script
local Mouse = Player:GetMouse()
local Cam = game.Workspace.CurrentCamera
local dir = {w = 0, s = 0, a = 0, d = 0}
local spd = 2

Mouse.KeyDown:Connect(function(key)
    if key:lower() == "[" then
        toggleGhostMode()
    elseif key:lower() == "]" then
        toggleTeleportToBody()
    end
    if GhostMode then
        if key:lower() == "w" then
            dir.w = 1
        elseif key:lower() == "s" then
            dir.s = 1
        elseif key:lower() == "a" then
            dir.a = 1
        elseif key:lower() == "d" then
            dir.d = 1
        elseif key:lower() == "q" then
            spd = spd + 1
        elseif key:lower() == "e" then
            spd = spd - 1
        end
    end
end)

Mouse.KeyUp:Connect(function(key)
    if GhostMode then
        if key:lower() == "w" then
            dir.w = 0
        elseif key:lower() == "s" then
            dir.s = 0
        elseif key:lower() == "a" then
            dir.a = 0
        elseif key:lower() == "d" then
            dir.d = 0
        end
    end
end)

-- Flying loop
while true do
    wait(1/44)
    if GhostMode then
        Root.CFrame = CFrame.new(Root.Position, Cam.CFrame.p)
            * CFrame.Angles(0, math.rad(180), 0)
            * CFrame.new((dir.d - dir.a) * spd, 0, (dir.s - dir.w) * spd)
    end
end


end)

SectionFE:NewButton("Flings yourself", "Fling yourself", function()

-- Get the executor's LocalPlayer
local player = game.Players.LocalPlayer

-- Function to fling the local player in a random direction
local function flingPlayer()
    local character = player.Character
    if character then
        local humanoid = character:FindFirstChildOfClass("Humanoid")
        if humanoid then
            -- Generate a random direction vector
            local randomDirection = Vector3.new(math.random(-10, 10), 20, math.random(-10, 10)).unit
            local flingForce = Instance.new("BodyVelocity")
            flingForce.Velocity = randomDirection * 1000
            flingForce.MaxForce = Vector3.new(100000, 100000, 100000)
            flingForce.P = 5000
            flingForce.Parent = character.HumanoidRootPart
            wait(0.1)
            flingForce:Destroy()
        end
    end
end

-- GUI setup with decoration
local gui = Instance.new("ScreenGui")
gui.Parent = game.Players.LocalPlayer.PlayerGui

local background = Instance.new("Frame")
background.Size = UDim2.new(0.2, 0, 0.15, 0)
background.Position = UDim2.new(0.75, 0, 0.1, 0)
background.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
background.BorderSizePixel = 0
background.Parent = gui

local header = Instance.new("TextLabel")
header.Size = UDim2.new(1, 0, 0.2, 0)
header.Position = UDim2.new(0, 0, 0, 0)
header.Text = "Player Flinger"
header.TextColor3 = Color3.fromRGB(255, 255, 255)
header.TextSize = 18
header.BackgroundTransparency = 1
header.Font = Enum.Font.SourceSansBold
header.Parent = background

local button = Instance.new("TextButton")
button.Parent = background
button.Size = UDim2.new(0.8, 0, 0.3, 0)
button.Position = UDim2.new(0.1, 0, 0.7, 0)
button.Text = "Fling Yourself!"
button.TextColor3 = Color3.fromRGB(255, 255, 255)
button.BackgroundColor3 = Color3.fromRGB(0, 120, 215)
button.Font = Enum.Font.SourceSans
button.TextSize = 16
button.BorderSizePixel = 0

-- Connect button click to flingPlayer function
button.MouseButton1Click:Connect(function()
    flingPlayer()
end)


end)

SectionFE:NewButton("Block Teleport", "Teleports you to block", function()

-- Block Placement and Teleportation Script
local UserInputService = game:GetService("UserInputService")
local Players = game:GetService("Players")
local Workspace = game:GetService("Workspace")
local Camera = Workspace.CurrentCamera

local player = Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

local blockSize = Vector3.new(4, 1, 4) -- Size of the block
local blockColor = BrickColor.new("Bright green") -- Color of the preview block
local blockMaterial = Enum.Material.SmoothPlastic -- Material of the block
local previewBlock = nil -- Reference to the preview block
local placedBlock = nil -- Reference to the placed block

-- Function to create or update the preview block
local function updatePreviewBlock(position)
    if not previewBlock then
        -- Create the preview block
        previewBlock = Instance.new("Part")
        previewBlock.Size = blockSize
        previewBlock.Anchored = true
        previewBlock.CanCollide = false
        previewBlock.BrickColor = blockColor
        previewBlock.Material = blockMaterial
        previewBlock.Transparency = 0.5 -- Semi-transparent
        previewBlock.Name = "PreviewBlock"
        previewBlock.Parent = Workspace
    end
    previewBlock.Position = position
end

-- Function to place the block
local function placeBlock(position)
    if previewBlock then
        -- Remove previous placed block if any
        if placedBlock then
            placedBlock:Destroy()
        end

        -- Create and place the actual block
        placedBlock = Instance.new("Part")
        placedBlock.Size = blockSize
        placedBlock.Position = position
        placedBlock.Anchored = true
        placedBlock.CanCollide = true
        placedBlock.BrickColor = BrickColor.new("Bright red") -- Color of the actual block
        placedBlock.Material = blockMaterial
        placedBlock.Name = "PlacedBlock"
        placedBlock.Parent = Workspace

        -- Remove the preview block
        previewBlock:Destroy()
        previewBlock = nil
    end
end

-- Function to handle mouse click to place the block
local function onMouseClick()
    if previewBlock then
        local mouse = player:GetMouse()
        local mousePosition = mouse.Hit.Position
        placeBlock(mousePosition)
    end
end

-- Function to teleport to the placed block
local function teleportToBlock()
    if placedBlock then
        humanoidRootPart.CFrame = CFrame.new(placedBlock.Position + Vector3.new(0, placedBlock.Size.Y / 2, 0))
    end
end

-- Function to handle key inputs
local function onInput(input, gameProcessedEvent)
    if gameProcessedEvent then return end -- Ignore inputs that the game is processing

    if input.KeyCode == Enum.KeyCode.Z then
        -- Show the preview block at the player's position
        updatePreviewBlock(humanoidRootPart.Position)
    elseif input.KeyCode == Enum.KeyCode.X then
        -- Teleport to the placed block
        teleportToBlock()
    end
end

-- Input event connection
UserInputService.InputBegan:Connect(onInput)

-- Mouse click event connection
UserInputService.InputBegan:Connect(function(input, gameProcessedEvent)
    if gameProcessedEvent then return end -- Ignore inputs that the game is processing

    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        onMouseClick()
    end
end)


end)

SectionFE:NewButton("Sleeping", "Lying Down", function()

-- Sleep Toggle Script with Side Lying Pose
local UserInputService = game:GetService("UserInputService")
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")

local player = Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

-- Sleep State Variables
local isAsleep = false

-- Function to make the character lie on their side with their arm on their head
local function fallAsleep()
    if isAsleep then return end

    isAsleep = true
    -- Make the character lie on their side
    humanoid.PlatformStand = true
    -- Adjust the CFrame to simulate lying on the side
    local position = humanoidRootPart.Position
    -- Define the CFrame to simulate the side lying posture
    humanoidRootPart.CFrame = CFrame.new(position) * CFrame.fromEulerAnglesXYZ(math.rad(90), 0, 0)
end

-- Function to wake the character up
local function wakeUp()
    if not isAsleep then return end

    isAsleep = false
    -- Restore character's movement and controls
    humanoid.PlatformStand = false
end

-- Toggle sleep state with the '[' key
UserInputService.InputBegan:Connect(function(input, gameProcessedEvent)
    if gameProcessedEvent then return end -- Ignore inputs that the game is processing

    if input.KeyCode == Enum.KeyCode.LeftBracket then
        if isAsleep then
            wakeUp()
        else
            fallAsleep()
        end
    end
end)

-- Keep updating the character's state on every frame
RunService.RenderStepped:Connect(function()
    if isAsleep then
        -- Ensure the character's orientation is maintained while lying down
        local position = humanoidRootPart.Position
        humanoidRootPart.CFrame = CFrame.new(position) * CFrame.fromEulerAnglesXYZ(math.rad(90), 0, 0)
    end
end)


end)

