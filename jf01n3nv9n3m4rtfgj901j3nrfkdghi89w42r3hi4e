ClientwServer:NewButton("Destroy Walls/Add", "Destroys/Adds walls", function()

local Player = game.Players.LocalPlayer
local Character = Player.Character or Player.CharacterAdded:Wait()
local UIS = game:GetService("UserInputService")

local removeWallsEnabled = false
local destroyedWalls = {} -- Table to keep track of destroyed walls

-- Function to determine if a part is directly below the character's feet
local function isFloor(part)
    local rayOrigin = Character.HumanoidRootPart.Position
    local rayDirection = Vector3.new(0, -5, 0) -- Ray going downward from the character
    local raycastParams = RaycastParams.new()
    raycastParams.FilterDescendantsInstances = {Character}
    raycastParams.FilterType = Enum.RaycastFilterType.Blacklist

    local raycastResult = workspace:Raycast(rayOrigin, rayDirection, raycastParams)

    return raycastResult and raycastResult.Instance == part
end

-- Function to handle touching parts
local function onTouched(part)
    if removeWallsEnabled and part:IsA("BasePart") and not isFloor(part) then
        destroyedWalls[part] = part:Clone() -- Store a clone of the destroyed part
        part:Destroy()
    end
end

-- Restore all destroyed walls
local function restoreWalls()
    for part, clone in pairs(destroyedWalls) do
        clone.Parent = workspace -- Reparent the clone back to the workspace
        clone.Position = part.Position -- Maintain the original position
        clone.Anchored = true -- Ensure the part stays in place
    end
    destroyedWalls = {} -- Clear the destroyed walls table
end

-- Connect the Touched event to all parts in the character
local function connectTouchedEvent()
    for _, part in pairs(Character:GetDescendants()) do
        if part:IsA("BasePart") then
            part.Touched:Connect(onTouched)
        end
    end
end

-- Toggle the removeWallsEnabled variable when Right Control is pressed
local function onToggleKeyPress(input, gameProcessed)
    if input.KeyCode == Enum.KeyCode.RightControl and not gameProcessed then
        removeWallsEnabled = not removeWallsEnabled
        if removeWallsEnabled then
            print("Wall removal enabled")
        else
            print("Wall removal disabled")
            restoreWalls() -- Restore walls when disabling removal
        end
    end
end

-- Connect the Touched event when the character spawns
Player.CharacterAdded:Connect(function(char)
    Character = char
    connectTouchedEvent()
end)

-- Connect the Touched event to the current character if it already exists
if Player.Character then
    connectTouchedEvent()
end

-- Connect the input event to toggle wall removal
UIS.InputBegan:Connect(onToggleKeyPress)

end)

